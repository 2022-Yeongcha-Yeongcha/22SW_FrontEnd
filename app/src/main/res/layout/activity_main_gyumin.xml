    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="64dp"
        android:layout_height="59dp"
        app:srcCompat="@drawable/sos_symbol"
        tools:layout_editor_absoluteX="332dp"
        tools:layout_editor_absoluteY="21dp" />

